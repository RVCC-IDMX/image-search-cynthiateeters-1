{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-trailing-spaces": "error",
    "semi": "error",
    "arrow-body-style": 0,
    "linebreak-style": [
      "error",
      "unix"
    ]
  }
}